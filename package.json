{
  "name": "jetbrains-open",
  "author": "Rocky Gray <rocky.grayjr@gmail.com>",
  "version": "0.2.0",
  "description": "Open Jetbrains IDEs from the command line",
  "repository": {
    "type": "git",
    "url": "https://github.com/grocky/jetbrains"
  },
  "bugs": {
    "url": "https://github.com/grocky/jetbrains/issues"
  },
  "homepage": "https://github.com/grocky/jetbrains#readme",
  "os": [
    "!win32"
  ],
  "main": "distribution/jetbrains.js",
  "types": "distribution/index.d.ts",
  "bin": {
    "jetbrains": "distribution/index.js"
  },
  "files": [
    "bin",
    "distribution/**/*"
  ],
  "scripts": {
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "jetbrains",
    "ide"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "4.2.3",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.12",
    "@types/shelljs": "0.8.5",
    "chai": "4.2.0",
    "mocha": "6.2.1",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "^2.0",
    "shelljs": "0.7.8"
  }
}
