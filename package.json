{
  "name": "jetbrains-open",
  "author": "Rocky Gray <rocky.grayjr@gmail.com>",
  "version": "0.4.0",
  "description": "Open Jetbrains IDEs from the command line",
  "repository": {
    "type": "git",
    "url": "https://github.com/grocky/jetbrains"
  },
  "bugs": {
    "url": "https://github.com/grocky/jetbrains/issues"
  },
  "homepage": "https://github.com/grocky/jetbrains#readme",
  "os": [
    "!win32"
  ],
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "jetbrains": "dist/src/index.js"
  },
  "scripts": {
    "build": "npm run clean && tsc -p .",
    "build:check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "prepare": "npm run build",
    "start": "ts-node src/index.ts",
    "test": "npm run lint && npm run test:unit && npm run test:coverage",
    "test:coverage": "nyc mocha",
    "test:unit": "mocha"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "src/index.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100
  },
  "keywords": [
    "jetbrains",
    "ide"
  ],
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@types/chai": "4.2.3",
    "@types/chalk": "2.2.0",
    "@types/debug": "4.1.5",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.12",
    "@types/shelljs": "0.8.5",
    "chai": "4.2.0",
    "mocha": "6.2.1",
    "nyc": "14.1.1",
    "prettier": "1.18.2",
    "source-map-support": "0.5.13",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "chalk": "2.4.2",
    "commander": "2.20.3",
    "debug": "4.1.1",
    "shelljs": "0.7.8"
  }
}
